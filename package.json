{
  "name": "noted-api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "build": "tsx ace build",
    "start": "node build/bin/server.js",
    "predev": "npm run build",
    "dev": "tsx ace serve --hmr",
    "test": "node ace test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "migration:run": "tsx ace migration:run",
    "migration:rollback": "tsx ace migration:rollback",
    "migration:status": "tsx ace migration:status"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#start/*": "./start/*.js",
    "#tests/*": "./tests/*.js",
    "#config/*": "./config/*.js",
    "#utils/*": "./app/utils/*.js",
    "#types/*": "./types/*.js",
    "#routers/*": "./app/routers/*.js",
    "#prompts/*": "./app/prompts/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/eslint-config": "^2.0.0",
    "@adonisjs/prettier-config": "^1.4.4",
    "@adonisjs/tsconfig": "^1.4.0",
    "@japa/api-client": "^3.1.0",
    "@japa/assert": "^4.0.1",
    "@japa/plugin-adonisjs": "^4.0.0",
    "@japa/runner": "^4.2.0",
    "@swc/core": "1.11.24",
    "@types/luxon": "^3.6.2",
    "@types/node": "^22.15.18",
    "@types/uuid": "10",
    "eslint": "^9.26.0",
    "hot-hook": "^0.4.0",
    "pino-pretty": "^13.1.1",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "ts-node-maintained": "^10.9.6",
    "tsx": "^4.20.5",
    "typescript": "~5.8"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.4.0",
    "@adonisjs/core": "^6.18.0",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/lucid": "^21.6.1",
    "@aws-sdk/client-s3": "^3.844.0",
    "@aws-sdk/s3-request-presigner": "^3.844.0",
    "@pinecone-database/pinecone": "^6.1.2",
    "@vinejs/vine": "^3.0.1",
    "jose": "^5.2.4",
    "luxon": "^3.6.1",
    "openai": "^5.12.2",
    "pdfjs-dist": "^5.4.54",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "uuid": "^11.1.0"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  },
  "prettier": "@adonisjs/prettier-config"
}
